      <article>
        <!-- support -->
        <div class="contact-form">
          <h1 class="h1">Add the History</h1>
          <div class="contain">
            <div class="main">
              <div class="content">
                <form action="/admin-history" method="POST">
                  <input type="tel" name="number" placeholder="What number" />
                  <select name="type" id="">
                    <option value="Bitcoin">
                      <i class="ri-btc-fill"></i> Bitcoin
                    </option>
                    <option value="ETH"><i class="ri-eth-fill"></i> ETH</option>
                    <option value="USDT">
                      <i class="ri-usdt-fill"></i> USDT
                    </option>
                    <option value="USDC">
                      <i class="ri-usdc-fill"></i> USDC
                    </option>
                  </select>
                  <input
                    type="text"
                    name="amount"
                    placeholder="Enter the amount"
                  />
                  <input
                    type="tid"
                    name="transactionId"
                    placeholder="Enter the id"
                  />
                  <input type="date" name="date" placeholder="Enter the date" />
                  <input
                    type="text"
                    name="action"
                    placeholder="Enter the action eg: sent or recieved"
                  />
                  <input
                    type="text"
                    name="status"
                    placeholder="Enter the status eg: successful or failed"
                  />
                  <button type="submit" class="btn">
                    Save <i class="fa-solid fa-save"></i>
                  </button>
                </form>
              </div>
              <div class="form-img">
                <img src="../images/thoughts.svg" alt="support" />
              </div>
            </div>
          </div>
        </div>
      </article>
