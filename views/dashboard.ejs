<!DOCTYPE html>
<html lang="en">
  <head>
    <%- include('includes/metadata') %>
    <title>Dashboard - Cryptex</title>
    <%- include('includes/dash-head') %>
  </head>
  <body>
    <!-- preloader -->
    <%- include('includes/preloader') %>
    <!-- ./ preloader -->

    <!-- menu -->
    <div class="menu">
      <div class="menu-header">
        <a href="/dashboard" class="menu-header-logo">
          <img src="../images/logo.svg" alt="logo" />
          <span>Cryptex</span>
        </a>
        <a href="#" class="btn btn-sm menu-close-btn">
          <i class="bi bi-x"></i>
        </a>
      </div>
      <div class="menu-body">
        <div class="dropdown">
          <a
            href=""
            class="d-flex align-items-center"
            data-bs-toggle="dropdown"
          >
            <div class="avatar me-3">
              <img
                src="../images/author.png"
                class="rounded-circle"
                alt="image"
              />
            </div>
            <div>
              <div class="fw-bold" style="color: black">Bella</div>
              <small class="text-danger">unverified</small>
            </div>
          </a>
          <div class="dropdown-menu dropdown-menu-end">
            <a
              href="/login"
              class="dropdown-item d-flex align-items-center text-danger"
            >
              <i class="bi bi-box-arrow-right dropdown-item-icon"></i> Logout
            </a>
          </div>
        </div>
        <ul>
          <li class="menu-divider">Dashboard</li>
          <li>
            <a class="active" href="/dashboard">
              <span class="nav-link-icon">
                <i class="ri-dashboard-line"></i>
              </span>
              <span>Dashboard</span>
            </a>
          </li>
          <li>
            <a href="/markets">
              <span class="nav-link-icon">
                <i class="bi bi-bar-chart"></i>
              </span>
              <span>Markets</span>
            </a>
          </li>
          <li>
            <a href="#">
              <span class="nav-link-icon">
                <i class="ri-bank-card-line"></i>
              </span>
              <span>Funds</span>
            </a>
            <ul>
              <li>
                <a href="/deposit">Deposit</a>
              </li>
              <li>
                <a href="/transfer">Transfer</a>
              </li>
              <li>
                <a href="/withdraw">Withdraw</a>
              </li>
            </ul>
          </li>
          <li>
            <a href="/history">
              <span class="nav-link-icon">
                <i class="ri-history-line"></i>
              </span>
              <span>History</span>
            </a>
          </li>
          <li>
            <a href="/packages">
              <span class="nav-link-icon">
                <i class="ri-box-3-line"></i>
              </span>
              <span>Packages</span>
              <span class="badge bg-success ms-auto">New</span>
            </a>
          </li>
          <li>
            <a href="/signal">
              <span class="nav-link-icon">
                <i class="ri-signal-tower-fill"></i>
              </span>
              <span>Signal</span>
            </a>
          </li>
          <li>
            <a href="/aml-kyc">
              <span class="nav-link-icon">
                <i class="ri-pass-valid-line"></i>
              </span>
              <span>AML/KYC</span>
            </a>
          </li>
          <li class="menu-divider">Pages</li>
          <li>
            <a href="/settings">
              <span class="nav-link-icon">
                <i class="bi bi-gear"></i>
              </span>
              <span>Settings</span>
            </a>
          </li>
        </ul>
      </div>
    </div>
    <!-- ./  menu -->

    <!-- layout-wrapper -->
    <div class="layout-wrapper">
      <!-- header -->
      <%- include('includes/dash-header') %>
      <!-- ./ header -->

      <!-- content -->
      <div class="content">
        <div class="row row-cols-1 row-cols-md-3 g-4">
          <div class="col-lg-12 col-md-12">
            <div class="card widget">
              <div class="card-body">
                <!-- TradingView Widget BEGIN -->
                <div class="tradingview-widget-container">
                  <div class="tradingview-widget-container__widget"></div>
                  <script
                    type="text/javascript"
                    src="https://s3.tradingview.com/external-embedding/embed-widget-ticker-tape.js"
                    async
                  >
                      {
                      "symbols": [
                        {
                          "proName": "BITSTAMP:BTCUSD",
                          "title": "Bitcoin"
                        },
                        {
                          "proName": "BITSTAMP:ETHUSD",
                          "title": "Ethereum"
                        },
                        {
                          "description": "BNB",
                          "proName": "CRYPTOCAP:BNB"
                        },
                        {
                          "description": "Solana",
                          "proName": "CRYPTOCAP:SOL"
                        },
                        {
                          "description": "XRP",
                          "proName": "CRYPTOCAP:XRP"
                        },
                        {
                          "description": "XTZ",
                          "proName": "BITFINEX:XTZUSD"
                        }
                      ],
                      "showSymbolLogo": true,
                      "colorTheme": "light",
                      "isTransparent": true,
                      "displayMode": "adaptive",
                      "locale": "en"
                    }
                  </script>
                </div>
                <!-- TradingView Widget END -->
              </div>
            </div>
          </div>
          <div class="col-lg-4 col-md-12">
            <div class="card widget">
              <div class="card-header d-flex">
                <h6 class="card-title">
                  Balance
                  <a
                    href="#"
                    class="bi bi-question-circle ms-1 small"
                    data-bs-toggle="tooltip"
                    title="Accumulating balance"
                  ></a>
                </h6>
              </div>
              <div class="card-body">
                <div class="d-md-flex align-items-center mb-3">
                  <div class="d-flex align-items-center">
                    <div class="display-7 me-3">$0.00</div>
                  </div>
                  <div class="d-flex align-items-center ms-auto mt-3 mt-lg-0">
                    <div class="display-7 me-3">
                      <i class="ri-wallet-line me-2 text-success"></i>
                    </div>
                  </div>
                </div>
              </div>
            </div>
          </div>
          <div class="col-lg-4 col-md-12">
            <div class="card widget">
              <div class="card-header d-flex">
                <h6 class="card-title">
                  Capital
                  <a
                    href="#"
                    class="bi bi-question-circle ms-1 small"
                    data-bs-toggle="tooltip"
                    title="Accumulating capital"
                  ></a>
                </h6>
              </div>
              <div class="card-body">
                <div class="d-md-flex align-items-center mb-3">
                  <div class="d-flex align-items-center">
                    <div class="display-7 me-3">$0.00</div>
                  </div>
                  <div class="d-flex align-items-center ms-auto mt-3 mt-lg-0">
                    <div class="display-7 me-3">
                      <i class="ri-arrow-left-right-line me-2 text-success"></i>
                    </div>
                  </div>
                </div>
              </div>
            </div>
          </div>
          <div class="col-lg-4 col-md-12">
            <div class="card widget">
              <div class="card-header d-flex">
                <h6 class="card-title">
                  Profit
                  <a
                    href="#"
                    class="bi bi-question-circle ms-1 small"
                    data-bs-toggle="tooltip"
                    title="Acquired profit"
                  ></a>
                </h6>
              </div>
              <div class="card-body">
                <div class="d-md-flex align-items-center mb-3">
                  <div class="d-flex align-items-center">
                    <div class="display-7 me-3">%0</div>
                  </div>
                  <div class="d-flex align-items-center ms-auto mt-3 mt-lg-0">
                    <div class="display-7 me-3">
                      <i class="ri-rocket-line me-2 text-success"></i>
                    </div>
                  </div>
                </div>
              </div>
            </div>
          </div>
          <div class="col-lg-12 col-md-12">
            <div class="card" style="height: 70vh">
              <div class="card-body">
                <div class="d-flex mb-3">
                  <div class="display-7">
                    <i class="ri-line-chart-line"></i>
                  </div>
                </div>
                <h4 class="mb-3">Chart</h4>
                <div class="d-flex mb-3">
                  <div
                    class="tradingview-widget-container"
                    style="height: 100%; width: 100%"
                  >
                    <div
                      id="tradingview_fb1d8"
                      style="height: 54vh; width: 100%"
                    ></div>
                    <script
                      type="text/javascript"
                      src="https://s3.tradingview.com/tv.js"
                    ></script>
                    <script type="text/javascript">
                      new TradingView.widget({
                        autosize: true,
                        symbol: "CRYPTOCAP:BTC",
                        interval: "D",
                        timezone: "Etc/UTC",
                        theme: "light",
                        style: "3",
                        locale: "en",
                        enable_publishing: false,
                        backgroundColor: "rgba(255, 255, 255, 1)",
                        allow_symbol_change: true,
                        save_image: false,
                        hide_volume: true,
                        container_id: "tradingview_fb1d8",
                      });
                    </script>
                  </div>
                </div>
              </div>
            </div>
          </div>
          <div class="col-lg-12 col-md-12">
            <div class="card">
              <div class="card-body">
                <div class="d-md-flex align-items-center mb-3">
                  <div class="avatar me-3">
                    <img
                      src="../images/refer.svg"
                      class="rounded-circle"
                      alt="image"
                    />
                  </div>
                  <div class="d-block align-items-center">
                    <div class="display-7 me-3">Refer Us & Earn</div>
                    <span> Use the link to invite your friends </span>
                  </div>
                  <div
                    class="d-flex gap-4 align-items-center ms-auto mt-3 mt-lg-0"
                  >
                    <div class="copy-text">
                      <input
                        type="text"
                        class="text"
                        value="https://filsugue.gihs/suubfugusb7367tsa7t7"
                      />
                      <button>
                        <i class="ri-file-copy-line"></i>
                      </button>
                    </div>
                  </div>
                </div>
              </div>
            </div>
          </div>
          <div class="col-lg-12 col-md-12">
            <div class="card">
              <div class="card-body">
                <div class="d-md-flex align-items-center mb-3">
                  <div class="d-block align-items-center">
                    <div class="display-7 me-3">We're here to help you!</div>
                    <span>
                      If you need to ask a question click on the button. Our
                      support team will get back to you via email.
                    </span>
                  </div>
                  <div
                    class="d-flex gap-4 align-items-center ms-auto mt-3 mt-lg-0"
                  >
                    <button
                      class="btn btn-primary btn-icon"
                      style="background-color: #3874ff; border: none"
                    >
                      Get Help
                    </button>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
      <!-- ./ content -->
      
      <%- include('includes/dash-footer') %>
  </body>
</html>
